hostname EDGE1


interface GigabitEthernet0/0
 no ip address
 shutdown
 duplex auto
 speed auto
!
interface GigabitEthernet0/1
 no ip address
 shutdown
 duplex auto
 speed auto
!
interface FastEthernet0/0/0
 no ip address
 duplex auto
 speed auto
!
interface FastEthernet0/0/0.30
 encapsulation dot1Q 30
 ip address 217.1.160.3 255.255.255.248
 ip nat inside
 ip virtual-reassembly in
!
interface FastEthernet0/0/0.100
 encapsulation dot1Q 100
 ip address 10.1.254.9 255.255.255.252
 ip nat inside
 ip virtual-reassembly in
!
interface FastEthernet0/0/0.300
 encapsulation dot1Q 300
 ip address 10.1.254.19 255.255.255.240
 ip nat inside
 ip virtual-reassembly in
 standby version 2 
 standby 10 ip 10.1.254.21
 standby 10 priority 110
 standby 10 preempt
!
interface FastEthernet0/1/0
 no ip address
 duplex auto
 speed auto
!
interface FastEthernet0/1/0.13
 encapsulation dot1Q 13
 ip address 10.1.254.6 255.255.255.252
!
interface FastEthernet0/1/0.14
 encapsulation dot1Q 14
 ip address 91.1.222.98 255.255.255.252
 ip nat outside
 ip virtual-reassembly in
!
router ospf 10
 redistribute connected subnets
 network 10.1.254.0 0.0.0.3 area 0
 network 10.1.254.4 0.0.0.3 area 0
 network 10.1.254.8 0.0.0.3 area 0
 network 10.1.254.16 0.0.0.15 area 0
!
router bgp 65001
 bgp router-id 1.1.1.2
 bgp log-neighbor-changes
  redistribute connected
  neighbor 91.1.222.97 remote-as 65000
  neighbor 91.1.222.97 activate
!
exit
!
ip nat inside source list NAT-IN interface FastEthernet0/1/0.14 overload
ip route 0.0.0.0 0.0.0.0 91.1.222.97
!
ip access-list standard NAT-IN
 permit 10.0.0.0 0.255.255.255
!
!
!
!
username admin privilege 15 secret P@ssw0rd
line vty 0 4
 login
 transport input ssh

end
